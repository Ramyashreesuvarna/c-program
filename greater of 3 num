#include <stdio.h>

int main() {
    int n,a,b,c,p,q,r;
    printf("enter n\n");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
    printf("enter values");
    scanf("%d%d%d\n",&a,&b,&c);
    p=a*b;
    q=a*c;
    r=b*c;
    if(p >q )
    {
        printf("\n%d",p);
    }
    else if(q>r){
        printf("\n%d",q);
    }
    else
    printf("\n%d",r);
}
    return 0;
}
